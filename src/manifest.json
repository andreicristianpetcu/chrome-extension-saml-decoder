{
  "manifest_version": 2,

  "name": "SAML Message Decoder",
  "description": "A Chrome extension that collects and displays SAML messages",
  "version": "0.0.1",

  "browser_action": {
    "default_popup": "web/index.html"
  },

  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },

  "permissions": [
    "<all_urls>","webRequest","webRequestBlocking"
  ],

  "background": {
    "page": "web/background/background.html"
  }
}